<!--pages/swiper/swiper.wxml-->
<!-- <text>swiper组件轮播图</text> -->
<swiper class='u-wrp-bnr' indicator-dots='true' autoplay='true' interval='5000' duration='500' circular='true'>
  <block wx:for="{{bnrUrl}}" wx:key="index">
    <swiper-item>
      <image src='{{item.url}}' class='u-img-slide' mode='aspectFill'></image>
    </swiper-item>
  </block>
</swiper>
